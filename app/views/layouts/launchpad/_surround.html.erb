<div class="launchpad-surround">
  <div class="search">
    <div class="bar">
      <input type="text" placeholder="Search anything..." data-behavior="search" tabindex="0" />
    </div>
  </div>
  <div class="top">
    <div class="top-left"></div>
    <div class="top-middle"></div>
    <div class="top-right"></div>
  </div>
  <div class="middle">
    <div class="left">
      <div class="right">
        <div class="content">
          <%= html %>
        </div>
      </div>
    </div>
  </div>
  <div class="bottom">
    <div class="bottom-left"></div>
    <div class="bottom-middle"></div>
    <div class="bottom-right"></div>
  </div>
  <div class="footer">
    <%= image_tag 'launchpad/logo.png' %>
  </div>
</div>
